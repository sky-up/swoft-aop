{
  "name": "swoft-fork/aop",
  "type": "library",
  "version": "v2.1.0",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "aop"
  ],
  "description": "swoft aop component",
  "homepage": "https://github.com/sky-up/swoft-aop",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.0",
    "swoft-fork/proxy": "~2.1.0",
    "swoft-fork/stdlib": "~2.1.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Aop\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Aop\\Unit\\": "test/unit/",
      "SwoftTest\\Aop\\Testing\\": "test/testing/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
